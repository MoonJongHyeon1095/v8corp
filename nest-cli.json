{
  "$schema": "https://json.schemastore.org/nest-cli",
  "collection": "@nestjs/schematics",
  "sourceRoot": "apps/v8corp/src",
  "compilerOptions": {
    "deleteOutDir": true,
    "webpack": true,
    "tsConfigPath": "apps/v8corp/tsconfig.app.json"
  },
  "projects": {
    "common": {
      "type": "library",
      "root": "libs/common",
      "entryFile": "index",
      "sourceRoot": "libs/common/src",
      "compilerOptions": {
        "tsConfigPath": "libs/common/tsconfig.lib.json"
      }
    },
    "v8corp": {
      "type": "application",
      "root": "apps/v8corp",
      "entryFile": "main",
      "sourceRoot": "apps/v8corp/src",
      "compilerOptions": {
        "tsConfigPath": "apps/v8corp/tsconfig.app.json"
      }
    },
    "info": {
      "type": "application",
      "root": "apps/info",
      "entryFile": "main",
      "sourceRoot": "apps/info/src",
      "compilerOptions": {
        "tsConfigPath": "apps/info/tsconfig.app.json"
      }
    }
  },
  "monorepo": true,
  "root": "apps/v8corp"
}